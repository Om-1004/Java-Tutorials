public class Main {

	public static void main(String[] args) {
		
		int age = 30;
		
		if (age > 18) {
			System.out.println("You are an adult");
		}
		else if(age>= 0 && age <= 17) {
			System.out.println("You are still a minor");
		}
		
		else {
			System.out.println("You are not alive");
		}
	}

}
