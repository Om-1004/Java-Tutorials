public class Main {

	public static void main(String[] args) {
		
		int money = 15;
		
		if (money >= 20) {
			System.out.println("You have more than enough to buy a PS5 game!");
		}
		
		else if(money == 15) {
			System.out.println("You have enough money to but the PS5 game!");
		}
		else {
			System.out.println("You don't have enough money to but the PS5 game!");
		}
	}

}
